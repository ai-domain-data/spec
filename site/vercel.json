{
  "rewrites": [
    {
      "source": "/((?!api|_next|assets|favicon|robots|sitemap|spec/schema).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/spec/schema-v0.1.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}

